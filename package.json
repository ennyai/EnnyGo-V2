{
  "name": "ennygo-v2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "node src/server/index.js",
    "prebuild": "node scripts/generate-env.js",
    "build": "vite build",
    "postbuild": "cp -r dist/* public/",
    "dev": "vite",
    "preview": "vite preview",
    "dev:client": "vite --port 3000",
    "dev:server": "nodemon src/server/index.js",
    "dev:all": "yarn predev:server && concurrently \"yarn dev:client\" \"yarn dev:server\"",
    "predev:server": "lsof -ti:3000,3001 | xargs kill -9 2>/dev/null || true",
    "webhook:init": "node src/server/scripts/initWebhook.js",
    "webhook:reset": "node src/server/scripts/reset-webhook.js",
    "cleanup": "node src/server/scripts/cleanup.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@reduxjs/toolkit": "^2.0.1",
    "@supabase/supabase-js": "^2.48.0",
    "axios": "^1.6.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "lucide-react": "^0.309.0",
    "morgan": "^1.10.0",
    "ngrok": "^5.0.0-beta.2",
    "nodemon": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.21.2",
    "recharts": "^2.15.0",
    "shadcn-ui": "^0.9.4",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@shadcn/ui": "^0.0.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "cross-fetch": "^4.1.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^24.0.0",
    "postcss": "^8.5.1",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.17",
    "vite": "^6.0.10",
    "vitest": "^0.32.0",
    "yargs": "^17.7.2"
  }
}
